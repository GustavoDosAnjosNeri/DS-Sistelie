<Window x:Class="DS_Sistelie.MenuInicial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DS_Sistelie"
        mc:Ignorable="d"
        Title="Menu Inicial" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="btnFornecedor" Margin="20 80 0 0" Height="50" Width="230" 
                    HorizontalAlignment="Center" FontSize="20" 
                    FontWeight="Bold" Background="#91c4c0" Click="btnFornecedor_Click">Cadastrar Fornecedor</Button>
        </StackPanel>

    </Grid>
</Window>
